    "test": "CHAINCODE_LOGGING_LEVEL=DEBUG jest"
  },
  "author": "SoftwareMill",
  "dependencies": {
    "fabric-contract-api": "^2.4.2",
    "fabric-shim": "^2.4.2"
  },
  "devDependencies": {
    "@theledger/fabric-mock-stub": "^5.0.1",
    "jest": "^26.6.3",
    "nodemon": "^2.0.20",
    "uuid": "^8.3.2"
  }
}
