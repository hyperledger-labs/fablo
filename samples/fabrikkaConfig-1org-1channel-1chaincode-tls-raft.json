{
  "$schema": "https://github.com/softwaremill/fablo/releases/download/0.3.0-unstable/schema.json",
  "networkSettings": {
    "fabricVersion": "1.4.6",
    "tls": true,
    "monitoring": {
      "loglevel": "debug"
    }
  },
  "orgs": [
    {
      "organization": {
        "name": "Orderer",
        "domain": "orderer.com"
      },
      "orderer": {
        "groupName": "group1",
        "prefix": "orderer",
        "type": "raft",
        "instances": 1
      }
    },
    {
      "organization": {
        "name": "Org1",
        "mspName": "Org1MSP",
        "domain": "org1.com"
      },
      "ca": {
        "prefix": "ca"
      },
      "peer": {
        "prefix": "peer",
        "instances": 2,
        "db": "LevelDb"
      }
    }
  ],
  "channels": [
    {
      "name": "my-channel1",
      "orgs": [
        {
          "name": "Org1",
          "peers": [
            "peer0"
          ]
        },
        {
          "name": "Org2",
          "peers": [
            "peer0",
            "peer1"
          ]
        }
      ]
    }
  ],
  "chaincodes": [
    {
      "name": "chaincode1",
      "version": "0.0.1",
      "lang": "node",
      "channel": "my-channel1",
      "init": "{\"Args\":[]}",
      "endorsement": "AND ('Org1MSP.member')",
      "directory": "./chaincodes/chaincode-kv-node-1.4"
    }
  ]
}
