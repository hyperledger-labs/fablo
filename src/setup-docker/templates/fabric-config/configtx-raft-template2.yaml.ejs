<% if (ordererGroup.consensus == 'etcdraft') { %>
    EtcdRaft:
        Consenters:<% ordererGroup.orderers.forEach(function(orderer) { %>
            - Host: <%= orderer.address %>
              Port: <%= orderer.port %>
              ClientTLSCert: crypto-config/ordererOrganizations/<%= orderer.domain %>/orderers/<%= orderer.address %>/tls/server.crt
              ServerTLSCert: crypto-config/ordererOrganizations/<%= orderer.domain %>/orderers/<%= orderer.address %>/tls/server.crt
<% })} -%>
