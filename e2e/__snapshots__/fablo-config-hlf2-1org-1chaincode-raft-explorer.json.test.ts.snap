    -o \\"$ORDERER_URL\\" \\\\
    -C \\"$CHANNEL_NAME\\" \\\\
    -n \\"$CHAINCODE_NAME\\" \\\\
    -v \\"$CHAINCODE_VERSION\\" \\\\
