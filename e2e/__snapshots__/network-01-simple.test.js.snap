    OrdererType: solo
    Addresses: 
        - orderer0.root.com:7050
    
    BatchTimeout: 2s
    BatchSize:
        MaxMessageCount: 10
        AbsoluteMaxBytes: 99 MB
        PreferredMaxBytes: 512 KB
    Organizations:

    


################################################################################
