                Organizations: 
                    - *Org1
    AllOrgChannel:
        <<: *ChannelDefaults
        Consortium: SampleConsortium
        Application:
            <<: *ApplicationDefaults
            Organizations: 
                - *Org1
            Capabilities:
                <<: *ApplicationCapabilities

"
