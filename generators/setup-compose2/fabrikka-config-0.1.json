{
  "fabrikka-version": "0.0.1-proto",
  "file-version": "0.1",
  "network-settings": {
    "fabric-version": "1.4.3",
    "tls": false,
    "single-ca": true,
    "single-cli": true
  },
  "root-org": {
    "organization": {
      "key": "root",
      "id": "OrdererMSP",
      "name": "Orderer",
      "domain": "example.com"
    },
    "ca": {
      "prefix": "root-ca"
    },
    "orderer": {
      "prefix": "orderer",
      "consensus": "solo",
      "instances": 1
    }
  },
  "orgs": [
    {
      "organization": {
        "key": "org1",
        "id": "Org1",
        "name": "Org1",
        "domain": "org1.com"
      },
      "ca": {
        "prefix": "ca"
      },
      "peer": {
        "prefix": "peer",
        "instances": 3,
        "db": "CouchDB"
      }
    }
  ],
  "channels": [
    {
      "key": "channel1",
      "name": "myChannel",
      "orgs": [
        {
          "key": "org1",
          "peers": [
            "peer0",
            "peer1"
          ]
        }
      ]
    }
  ],
  "chaincodes": [
    {
      "channel": "channel1",
      "name": "chaincode1",
      "version": "0.0.1",
      "lang": "java"
    }
  ]
}
