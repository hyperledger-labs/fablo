{
  "generator-fabric-network": {
    "file-version": "0.1",
    "fabrikka-version": "0.0.1-proto",
    "network-settings": {
      "fabric-version": "1.4.3",
      "tls": false,
      "single-ca": true,
      "single-cli": true
    },
    "root-org": {
      "organization": {
        "key": "root",
        "id": "OrdererMSP",
        "name": "Orderer",
        "domain": "example.com"
      },
      "ca": {
        "prefix": "root-ca"
      },
      "orderer": {
        "prefix": "orderer",
        "instances": 1,
        "consensus": "solo"
      }
    },
    "orgs": [
      {
        "organization": {
          "key": "org1",
          "id": "Org1",
          "name": "Org1",
          "domain": "org1.com"
        },
        "ca": {
          "prefix": "ca"
        },
        "peer": {
          "prefix": "peer",
          "instances": 3,
          "db": "CouchDB"
        }
      }
    ],
    "channel-consortium": {
      "orgs": [
        "root-org",
        "org1"
      ]
    },
    "channels": [
      {
        "key": "channel1",
        "name": "myChannel",
        "orgs": [
          {
            "key": "org1",
            "peers": ["peer0", "peer1"]
          }
        ]
      }
    ]
  }
}
